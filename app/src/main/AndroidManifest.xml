<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.DicodingAndroid">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.LearningProjectDicoding">
        <activity
            android:name="com.example.DicodingAndroid.SplashScreenActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.DicodingAndroid.DetailView"
            android:parentActivityName="com.example.DicodingAndroid.DetailView" />
        <activity
            android:name="com.example.DicodingAndroid.profil"
            android:parentActivityName="com.example.DicodingAndroid.profil" />
        <activity
            android:name="com.example.DicodingAndroid.MainActivity"
            android:exported="true">
        </activity>
    </application>

</manifest>